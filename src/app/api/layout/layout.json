        "id": "876fa831-23cc-44d9-a7d1-c0c0e8c4e20f",
        "code": "({ \n  logoSrc = 'https://global-cloud-logo.png', \n  logoAlt = 'Global Cloud Logo', \n  navItem1Text = 'Services', \n  navItem2Text = 'Solutions', \n  navItem3Text = 'Pricing', \n  navItem4Text = 'Support',\n  ctaText = 'Get Started',\n  ctaHref = '/signup'\n}) => {\n  const [mobileMenuOpen, setMobileMenuOpen] = React.useState(false);\n\n  return (\n    <header className=\"w-full bg-white dark:bg-[#121826] shadow-sm fixed top-0 left-0 z-50\">\n      <div className=\"container mx-auto px-4 py-4 flex items-center justify-between\">\n        {/* Logo */}\n        <a href=\"/\" className=\"flex items-center\">\n          <img \n            src={logoSrc} \n            alt={logoAlt} \n            className=\"h-10 w-auto\"\n          />\n        </a>\n\n        {/* Desktop Navigation */}\n        <nav className=\"hidden md:flex items-center space-x-6\">\n          <a \n            href=\"#services\" \n            className=\"text-[#0A6EBD] hover:text-[#10B981] transition-colors text-base font-medium\"\n          >\n            {navItem1Text}\n          </a>\n          <a \n            href=\"#solutions\" \n            className=\"text-[#0A6EBD] hover:text-[#10B981] transition-colors text-base font-medium\"\n          >\n            {navItem2Text}\n          </a>\n          <a \n            href=\"#pricing\" \n            className=\"text-[#0A6EBD] hover:text-[#10B981] transition-colors text-base font-medium\"\n          >\n            {navItem3Text}\n          </a>\n          <a \n            href=\"#support\" \n            className=\"text-[#0A6EBD] hover:text-[#10B981] transition-colors text-base font-medium\"\n          >\n            {navItem4Text}\n          </a>\n        </nav>\n\n        {/* CTA Button */}\n        <div className=\"hidden md:flex items-center space-x-4\">\n          <a \n            href={ctaHref} \n            className=\"bg-[#0A6EBD] text-white px-4 py-2 rounded-md hover:bg-[#10B981] transition-colors font-medium\"\n          >\n            {ctaText}\n          </a>\n        </div>\n\n        {/* Mobile Menu Toggle */}\n        <div className=\"md:hidden\">\n          <button \n            onClick={() => setMobileMenuOpen(!mobileMenuOpen)}\n            className=\"text-[#0A6EBD] focus:outline-none\"\n            aria-label=\"Toggle mobile menu\"\n          >\n            <svg \n              xmlns=\"http://www.w3.org/2000/svg\" \n              className=\"h-6 w-6\" \n              fill=\"none\" \n              viewBox=\"0 0 24 24\" \n              stroke=\"currentColor\"\n            >\n              {mobileMenuOpen ? (\n                <path \n                  strokeLinecap=\"round\" \n                  strokeLinejoin=\"round\" \n                  strokeWidth={2} \n                  d=\"M6 18L18 6M6 6l12 12\" \n                />\n              ) : (\n                <path \n                  strokeLinecap=\"round\" \n                  strokeLinejoin=\"round\" \n                  strokeWidth={2} \n                  d=\"M4 6h16M4 12h16M4 18h16\" \n                />\n              )}\n            </svg>\n          </button>\n        </div>\n      </div>\n\n      {/* Mobile Menu */}\n      {mobileMenuOpen && (\n        <div className=\"md:hidden fixed inset-0 bg-white dark:bg-[#121826] z-40 pt-20\">\n          <nav className=\"px-4 space-y-4\">\n            <a \n              href=\"#services\" \n              className=\"block text-[#0A6EBD] hover:text-[#10B981] py-2 text-xl\"\n              onClick={() => setMobileMenuOpen(false)}\n            >\n              {navItem1Text}\n            </a>\n            <a \n              href=\"#solutions\" \n              className=\"block text-[#0A6EBD] hover:text-[#10B981] py-2 text-xl\"\n              onClick={() => setMobileMenuOpen(false)}\n            >\n              {navItem2Text}\n            </a>\n            <a \n              href=\"#pricing\" \n              className=\"block text-[#0A6EBD] hover:text-[#10B981] py-2 text-xl\"\n              onClick={() => setMobileMenuOpen(false)}\n            >\n              {navItem3Text}\n            </a>\n            <a \n              href=\"#support\" \n              className=\"block text-[#0A6EBD] hover:text-[#10B981] py-2 text-xl\"\n              onClick={() => setMobileMenuOpen(false)}\n            >\n              {navItem4Text}\n            </a>\n            <a \n              href={ctaHref} \n              className=\"block bg-[#0A6EBD] text-white px-6 py-3 rounded-md text-center text-xl hover:bg-[#10B981] transition-colors\"\n              onClick={() => setMobileMenuOpen(false)}\n            >\n              {ctaText}\n            </a>\n          </nav>\n        </div>\n      )}\n    </header>\n  );\n}",
