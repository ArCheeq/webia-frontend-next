        "id": "3a909e74-8ae5-495b-87e5-2c2bbaeb0a45",
        "code": "({\n  logo1Src = '/logo.svg', \n  companyName1Text = 'Global Cloud', \n  description1Text = 'Reliable, Scalable, Innovative Cloud Infrastructure',\n  link1Text = 'Products', \n  link1Href = '/products',\n  link2Text = 'Services', \n  link2Href = '/services',\n  link3Text = 'About', \n  link3Href = '/about',\n  socialLink1Href = 'https://twitter.com/globalcloud',\n  socialLink2Href = 'https://linkedin.com/globalcloud',\n  socialLink3Href = 'https://github.com/globalcloud',\n  newsletterTitle1Text = 'Stay Updated',\n  newsletterSubtitle1Text = 'Subscribe to our newsletter',\n  newsletterPlaceholder1Text = 'Enter your email',\n  copyrightText = '© 2023 Global Cloud. All Rights Reserved.',\n}) => {\n  const [email, setEmail] = React.useState('');\n\n  const handleNewsletterSubmit = (e) => {\n    e.preventDefault();\n    // Newsletter submission logic\n  };\n\n  return (\n    <footer className=\"w-full bg-gray-900 text-white py-12 px-4 md:px-8 lg:px-16\">\n      <div className=\"max-w-7xl mx-auto grid grid-cols-1 md:grid-cols-4 gap-8\">\n        {/* Company Info */}\n        <div className=\"space-y-4\">\n          <img src={logo1Src} alt={`${companyName1Text} Logo`} className=\"h-10 w-auto\" />\n          <h3 className=\"text-lg font-semibold\">{companyName1Text}</h3>\n          <p className=\"text-gray-300\">{description1Text}</p>\n        </div>\n\n        {/* Quick Links */}\n        <div>\n          <h4 className=\"text-lg font-semibold mb-4\">Quick Links</h4>\n          <nav className=\"space-y-2\">\n            <a href={link1Href} className=\"block text-gray-300 hover:text-white transition-colors\">\n              {link1Text}\n            </a>\n            <a href={link2Href} className=\"block text-gray-300 hover:text-white transition-colors\">\n              {link2Text}\n            </a>\n            <a href={link3Href} className=\"block text-gray-300 hover:text-white transition-colors\">\n              {link3Text}\n            </a>\n          </nav>\n        </div>\n\n        {/* Social Links */}\n        <div>\n          <h4 className=\"text-lg font-semibold mb-4\">Connect</h4>\n          <div className=\"flex space-x-4\">\n            <a \n              href={socialLink1Href} \n              target=\"_blank\" \n              rel=\"noopener noreferrer\" \n              className=\"text-gray-300 hover:text-white transition-colors\"\n            >\n              <Icon icon=\"mdi:twitter\" className=\"h-6 w-6\" />\n            </a>\n            <a \n              href={socialLink2Href} \n              target=\"_blank\" \n              rel=\"noopener noreferrer\" \n              className=\"text-gray-300 hover:text-white transition-colors\"\n            >\n              <Icon icon=\"mdi:linkedin\" className=\"h-6 w-6\" />\n            </a>\n            <a \n              href={socialLink3Href} \n              target=\"_blank\" \n              rel=\"noopener noreferrer\" \n              className=\"text-gray-300 hover:text-white transition-colors\"\n            >\n              <Icon icon=\"mdi:github\" className=\"h-6 w-6\" />\n            </a>\n          </div>\n        </div>\n\n        {/* Newsletter */}\n        <div>\n          <h4 className=\"text-lg font-semibold mb-4\">{newsletterTitle1Text}</h4>\n          <p className=\"text-gray-300 mb-4\">{newsletterSubtitle1Text}</p>\n          <form onSubmit={handleNewsletterSubmit} className=\"flex\">\n            <input \n              type=\"email\"\n              value={email}\n              onChange={(e) => setEmail(e.target.value)}\n              placeholder={newsletterPlaceholder1Text}\n              className=\"w-full px-3 py-2 bg-gray-800 text-white rounded-l-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n            />\n            <button \n              type=\"submit\" \n              className=\"bg-blue-600 text-white px-4 py-2 rounded-r-md hover:bg-blue-700 transition-colors\"\n            >\n              <Icon icon=\"mdi:send\" className=\"h-5 w-5\" />\n            </button>\n          </form>\n        </div>\n      </div>\n\n      {/* Copyright */}\n      <div className=\"border-t border-gray-800 mt-8 pt-6 text-center text-gray-400\">\n        {copyrightText}\n      </div>\n    </footer>\n  );\n}",
